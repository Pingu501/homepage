{
  "name": "homepage",
  "version": "0.1.0",
  "dependencies": {
    "classnames": "^2.2.5",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.14",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "reselect": "^3.0.1"
  },
  "scripts": {
    "build-css": "node-sass ./sass/main.scss -o ./src/",
    "watch-css": "npm run build-css && node-sass ./sass/main.scss -o ./src/ -w -r",
    "build-js": "react-scripts build",
    "watch-js": "react-scripts start",
    "dev": "npm-run-all -p watch-css watch-js",
    "build": "npm run build-css && npm run build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
